<Window x:Class="EZBudget.PopupForms.ImageFullScreenViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EZBudget.PopupForms"
        mc:Ignorable="d"
        Title="ImageFullScreenViewerWindow" Height="500" Width="800">
    <Grid Background="{StaticResource BackgroundMidDarkGrayBrush}">
        <Image x:Name="Image" Source="{Binding ImageURL}"/>
    </Grid>
</Window>
